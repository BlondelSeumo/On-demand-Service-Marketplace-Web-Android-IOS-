<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <rotate android:fromDegrees="45">
            <shape>
                <size
                    android:width="8dp"
                    android:height="8dp" />
                <gradient
                    android:endColor="#ff9c00"
                    android:startColor="#ff9c00" />
            </shape>
        </rotate>
    </item>
</layer-list>